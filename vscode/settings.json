{
    // Workbench
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#255258",
        "editorCursor.foreground": "#00c359",
        "editor.selectionHighlightBackground": "#255258"
    },
    //editor
    "editor.formatOnSave": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorWidth": 3,
    "editor.cursorSmoothCaretAnimation": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "breadcrumbs.enabled": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "window.zoomLevel": 0,
    "files.associations": {
        "aliases": "shellscript",
        "func": "shellscript"
    },
    //Shell
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "git.confirmSync": false,
    // GIT
    "git.alwaysSignOff": true,
    "git.autofetch": true,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": true,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.advanced.telemetry.enabled": false,
    // GO
    "go.gopath": "~/Dev/go",
    "go.toolsGopath": "~/Dev/go",
    "go.liveErrors": {
        "enabled": true,
        "delay": 500
    },
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast",
        "--enable=unparam",
        "--enable=goimports",
        "--enable=interfacer",
    ],
    "go.autocompleteUnimportedPackages": true,
    "go.gocodeAutoBuild": true,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.formatTool": "goimports",
    // go editor settings
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
            "source.fixAll": true,
        },
    },
    // go modules
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "go.testTimeout": "3m",
    "go.useLanguageServer": true,
    "go.alternateTools": {
        "go-langserver": "gopls", // or bingo
    },
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true,
        "rename": true,
        "goToDefinition": true,
        "hover": true,
        "signatureHelp": true,
        "goToTypeDefinition": true,
        "goToImplementation": true,
        "documentSymbols": true,
        "workspaceSymbols": true,
        "findReferences": true,
        "diagnostics": true,
        "documentLink": true
    },
    "gopls": {
        "usePlaceholders": true, // add parameter placeholders when completing a function
        // Experimental settings
        "completeUnimported": true, // autocomplete unimported packages
        "buildFlags": [
            "-tags=wireinject"
        ],
        "experimentalWorkspaceModule": true,
    },
    // Notes
    "vsnotes.defaultNotePath": "/Users/d073003/Dev/vsnotes",
    "vsnotes.defaultNoteTitle": "{title}",
    "vsnotes.defaultSnippet": {
        "langId": "null",
        "name": "null,"
    },
    // emoji
    "emojisense.languages": {
        "markdown": true,
        "plaintext": {
            "markupCompletionsEnabled": false,
            "emojiDecoratorsEnabled": false
        },
        "go": {
            "markupCompletionsEnabled": false,
            "emojiDecoratorsEnabled": false,
            "unicodeCompletionsEnabled": true
        },
        "git-commit": false
    },
    // Terrafrm
    "terraform.telemetry.enabled": false,
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "external": true,
        "args": [
            "serve"
        ]
    },
    // TODO Tree    
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.tags": [
        "BUG",
        "FIXME",
        "TODO",
        "XXX",
        "BONFIRE",
        "WTF",
        "WIP"
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "iconColour": "#008080", // teal
            "background": "#008080", // teal
            "foreground": "#000099" // dark blue
        },
        "BUG": {
            "icon": "bug",
            "iconColour": "#7d00a0",
            "background": "#7d00a0",
            "foreground": "#5bf763"
        },
        "WTF": {
            "icon": "stop",
            "iconColour": "red",
            "background": "red",
            "foreground": "yellow"
        },
        "BONFIRE": {
            "icon": "flame",
            "iconColour": "#c63703",
            "foreground": "#efc11c",
            "background": "#c63703"
        },
        "FIXME": {
            "icon": "zap",
            "iconColour": "yellow",
            "background": "yellow",
            "foreground": "black"
        },
    },
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path.mac": "/Users/d073003/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
}